<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tzb.mapper.DeepSeekMapper">

    <select id="getApiKeyById" resultType="java.lang.String">
        SELECT configValue FROM config WHERE id = 14
    </select>

    <select id="getApiKeyByKey" resultType="java.lang.String">
        SELECT configValue FROM config WHERE configKey = '11'
    </select>

    <select id="getConfigById" resultType="tzb.pojo.DeepSeekConfig">
        SELECT
            id,
            configValue as apiKey
        FROM config
        WHERE id = #{id}
    </select>
</mapper>